      <bpmn2:outgoing>SequenceFlow_1</bpmn2:outgoing>
    </bpmn2:startEvent>
    <bpmn2:serviceTask id="UserTask_1" name="save order"
      camunda:delegateExpression="${saveOrderDelegate}">
      <bpmn2:incoming>SequenceFlow_1</bpmn2:incoming>
      <bpmn2:outgoing>Flow_1e0mz4u</bpmn2:outgoing>
    </bpmn2:serviceTask>
    <bpmn2:businessRuleTask id="Activity_00b6uap" name="Discount Type DMN Decision"
      camunda:decisionRef="discountDecision"
      camunda:mapDecisionResult="singleEntry"
      camunda:resultVariable="reduction">
      <bpmn2:incoming>Flow_1e0mz4u</bpmn2:incoming>
      <bpmn2:outgoing>Flow_1tth5t5</bpmn2:outgoing>
    </bpmn2:businessRuleTask>
    <bpmn2:exclusiveGateway id="ExclusiveGateway_1" name="check payment mode">
      <bpmn2:incoming>Flow_1tth5t5</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_3</bpmn2:outgoing>
      <bpmn2:outgoing>SequenceFlow_6</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:serviceTask id="ServiceTask_1" name="capture payment"
      camunda:delegateExpression="${capturePaymentDelegate}">
      <bpmn2:incoming>SequenceFlow_3</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_4</bpmn2:outgoing>
    </bpmn2:serviceTask>
    <bpmn2:exclusiveGateway id="ExclusiveGateway_2" name="check payment succeeded">
      <bpmn2:incoming>SequenceFlow_4</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_8</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_0dka0zk</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:serviceTask id="ServiceTask_2" name="mark as to pay at delivery"
      camunda:delegateExpression="${markAsToPayAtDeliveryDelegate}">
      <bpmn2:incoming>SequenceFlow_6</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_7</bpmn2:outgoing>
    </bpmn2:serviceTask>
    <bpmn2:userTask id="UserTask_2" name="prepare order">
      <bpmn2:incoming>SequenceFlow_8</bpmn2:incoming>
      <bpmn2:incoming>SequenceFlow_7</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_9</bpmn2:outgoing>
    </bpmn2:userTask>
    <bpmn2:serviceTask id="ServiceTask_3" name="ship order"
      camunda:delegateExpression="${shipOrderDelegate}">
      <bpmn2:incoming>SequenceFlow_9</bpmn2:incoming>
      <bpmn2:outgoing>Flow_0e9z3vz</bpmn2:outgoing>
    </bpmn2:serviceTask>
    <bpmn2:serviceTask id="Activity_07aolno" name="cancel order"
      camunda:delegateExpression="${cancelOrderDelegate}">
      <bpmn2:incoming>Flow_0dka0zk</bpmn2:incoming>
      <bpmn2:outgoing>Flow_1sre52g</bpmn2:outgoing>
    </bpmn2:serviceTask>
    <bpmn2:endEvent id="Event_0dvn7nx" name="order cancelled">
      <bpmn2:incoming>Flow_1sre52g</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:endEvent id="Event_0c7c4qs" name="order shipped">
      <bpmn2:incoming>Flow_0e9z3vz</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="SequenceFlow_1" sourceRef="StartEvent_1" targetRef="UserTask_1" />
    <bpmn2:sequenceFlow id="Flow_1e0mz4u" sourceRef="UserTask_1" targetRef="Activity_00b6uap" />
    <bpmn2:sequenceFlow id="Flow_1tth5t5" sourceRef="Activity_00b6uap"
      targetRef="ExclusiveGateway_1" />
    <bpmn2:sequenceFlow id="SequenceFlow_3" name="Online Payment" sourceRef="ExclusiveGateway_1"
      targetRef="ServiceTask_1">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${onlinePayment == true}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    <bpmn2:sequenceFlow id="SequenceFlow_6" sourceRef="ExclusiveGateway_1" targetRef="ServiceTask_2">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${onlinePayment == false}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    <bpmn2:sequenceFlow id="SequenceFlow_4" sourceRef="ServiceTask_1" targetRef="ExclusiveGateway_2" />
    <bpmn2:sequenceFlow id="SequenceFlow_7" sourceRef="ServiceTask_2" targetRef="UserTask_2" />
    <bpmn2:sequenceFlow id="SequenceFlow_8" name="payment successfull"
      sourceRef="ExclusiveGateway_2" targetRef="UserTask_2">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression">${paymentValidated == true}</bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
    <bpmn2:sequenceFlow id="Flow_0dka0zk" name="payment failed" sourceRef="ExclusiveGateway_2"
      targetRef="Activity_07aolno" />
    <bpmn2:sequenceFlow id="SequenceFlow_9" sourceRef="UserTask_2" targetRef="ServiceTask_3" />
    <bpmn2:sequenceFlow id="Flow_0e9z3vz" sourceRef="ServiceTask_3" targetRef="Event_0c7c4qs" />
    <bpmn2:sequenceFlow id="Flow_1sre52g" sourceRef="Activity_07aolno" targetRef="Event_0dvn7nx" />
  </bpmn2:process>
